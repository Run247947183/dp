# dp
动态规划
class Solution {
    // 动态规划
    // cash 表示不持有股票的最大化收益
    // hold 表示持有股票的最大化收益
    public int maxProfit(int[] prices, int fee) {
        int cash = 0;
        int hold = -prices[0];
        for (int i = 1; i < prices.length; i++) {
            cash = Math.max(cash, hold + prices[i] - fee);
            hold = Math.max(hold, cash - prices[i]);
        }
        return cash;
    }
}
